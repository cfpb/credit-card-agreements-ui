    "connect-history-api-fallback": "1.4.0",
    "core-js": "^2.5.1",
    "coveralls": "^3.0.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.7",
    "detect-port": "1.2.1",
    "dotenv": "4.0.0",
