    "cf-core": "^4.2.0",
    "cf-expandables": "^4.1.2",
    "cf-forms": "^4.2.0",
    "cf-grid": "^4.1.0",
    "cf-icons": "^4.1.0",
    "cf-layout": "^4.2.0",
    "cf-pagination": "^4.1.1",
    "cf-tables": "^4.1.1",
    "cf-typography": "^4.1.0",
    "history": "^4.6.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-modal": "^3.0.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
